<form {{action "authenticate" on="submit"}}>
  <p>
    <label>
      Username:
      {{input id="identification" type="text" value=identification}}
    </label>
  </p>
  <p>
    <label>
      Password:
      {{input id="password" type="password" value=password}}
    </label>
  </p>
  <p>
    <input type="submit" value="Log in"/>
  </p>
</form>
